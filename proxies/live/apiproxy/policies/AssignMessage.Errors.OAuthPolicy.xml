<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage name="AssignMessage.Errors.OauthPolicy">
    <AssignVariable>
        <Name>service.error.code</Name>
        <Ref>error.status.code</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>service.error.coding.code</Name>
        <Value>ACCESS_DENIED</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>service.error.coding.display</Name>
        <Value>Invalid access token</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>service.error.diagnostics</Name>
        <Ref>faultstring</Ref>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</AssignMessage>